fn for_loop() int {
    var x: int = 99;
    for var i: int = 0; i < 10; i = i + 1 {
        for var j: int = i; j < 10; j = j + 1 {
            printf("%d ", j);
        }
        printf("\n");
    }
    var y: int = 123;
    return x + y;
}

fn while_loop() void {
    var j: int = 0;
    while j < 10 {
        printf("while: %d\n", j);
        j = j + 1;
    }
}

fn main() int {

    var g: int = for_loop();

    return g;
}