include "util"

fn p_int(x: *int) void {
    printf("%d ", *x);
}

fn main() int {
    var ll: *LinkedList = new(LinkedList);
    ll..init();

    for i := 0; i < 10; ++i {
        ll..append(i);
    }

    ll..print(p_int);

    printf("size %d\n", ll..size);

    while ll..size > 0 {
        v := ll..pop();
        printf("%d\n", v);
    }

    return 0;
}