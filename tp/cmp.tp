struct Test {
    var x: int;
    var set: fn;
    var get: fn;
}

fn Test::set(x: int) void {
    this..x = x;
    printf("Set to %d\n", this..x);
}

fn Test::get() int {
    return this..x;
}

fn main(argc: int, argv: **char) int {
    var g: *Test = new(Test);
    g..set(argc);
    printf("%d\n", g..x);
    return g..get();
}
